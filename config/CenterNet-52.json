{
    "system": {
        "dataset": "CityPerson",
        "batch_size": 12,
        "sampling_function": "kp_detection",

        "train_split": "trainval",
        "val_split": "minival",

        "learning_rate": 0.00020,
        "decay_rate": 10,

        "val_iter": 500,

        "opt_algo": "adam",
        "prefetch_size": 20,

        "max_iter": 50000,
        "stepsize": 35000,
        "snapshot": 1000,

        "chunk_sizes": [6, 6],

        "data_dir": "/data/zwzhou/Data/CityPerson"
    },
    
    "db": {
        "rand_scale_min": 0.6,
        "rand_scale_max": 1.4,
        "rand_scale_step": 0.1,
        "rand_scales": null,

        "rand_crop": true,
        "rand_color": true,

        "border": 128,
        "gaussian_bump": true,

        "input_size": [511, 511],
        "output_sizes": [[128, 128]],

        "test_scales": [1],

        "top_k": 70,
        "categories": 1,
        "kp_categories": 1,
        "ae_threshold": 0.5,
        "nms_threshold": 0.5,

        "max_per_image": 100,
        "pretrained_model": "cache/nnet/CenterNet-52/CenterNet-52_480000.pkl"

    }
}
